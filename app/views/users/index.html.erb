<% provide(:title,'All users') %>
<h1>All users</h1>

<%# @usersはpaginateメソッドを使用した結果である必要がある %>
<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size:50 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
